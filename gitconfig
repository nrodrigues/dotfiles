[push]
    default = current
[alias]
    ul = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)%aN <%aE>%Creset' --abbrev-commit --date=relative
